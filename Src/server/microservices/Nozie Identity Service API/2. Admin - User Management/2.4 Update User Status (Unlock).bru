meta {
  name: 2.4 Update User Status (Unlock)
  type: http
  seq: 4
}

put {
  url: {{baseUrl}}/api/admin/users/{{userId}}/status
  body: json
  auth: inherit
}

headers {
  Content-Type: application/json
  Authorization: Bearer {{adminToken}}
}

body:json {
  {
    "status": "ACTIVE"
  }
}

settings {
  encodeUrl: true
}
